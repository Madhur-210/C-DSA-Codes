#include <iostream>
using namespace std;

void print2DArray(int **input, int row, int col) {
	// Write your code here
    int n=row,count=0;
    for(int i=0;i<row;i++){
        count=0;
        while(n-i>count){
        for(int j=0;j<col;j++){
            cout<<input[i][j]<<" ";
        }
           count++; 
        cout<<endl;
    }
        cout<<endl;
    }

}


