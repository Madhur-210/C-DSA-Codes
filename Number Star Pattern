#include<iostream>
using namespace std;


int main(){

  // Write your code here
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=2*n;j++){
            if(j<=n-i+1){
                cout<<j;
            }
            else if(j>n+i-1){
                cout<<2*n-j+1;
            }
            else{
                cout<<"*";
            }
        }
        cout<<endl;
    }

}


