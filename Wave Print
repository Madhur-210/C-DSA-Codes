void wavePrint(int **input, int nRows, int mCols)
{
    //Write your code here
    int j=0,i=0;
    while(j<mCols){
            if(j%2==0){
                for(i=0;i<nRows;i++){
                    cout<<input[i][j]<<" ";
                }
            }
            else{
                for(i=nRows-1;i>=0;i--){
                    cout<<input[i][j]<<" ";
                }
            }
        j++;
    }
}
