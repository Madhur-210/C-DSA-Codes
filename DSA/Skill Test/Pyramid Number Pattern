#include<iostream>
using namespace std;

int main(){

  // Write your code here  
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        int print = i;
        for(int j=1;j<n+i;j++){
            if(j>n-i){
                if(j<n)
                cout<<print--;
                else
                    cout<<print++;
            }
            else if(j<=n-i){
                cout<<" ";
            }
        }
        cout<<endl;
    }
  
}

