#include<queue>
int buyTicket(int *arr, int n, int k) {
    // Write your code here
    queue<int> q ;
    priority_queue<int> pq;
    for(int i=0;i<n;i++){
        pq.push(arr[i]);
        q.push(i);
    }
    int ans=0;
    while(!q.empty()){
        if(arr[q.front()]>=pq.top()){
            if(q.front()==k){
                return ans+1;
            }
            q.pop();
            pq.pop();
            ans++;
        }
        else{
            q.push(q.front());
            q.pop();
        }
    }
}
