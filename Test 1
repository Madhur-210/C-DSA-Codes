#include<iostream>
using namespace std;

int main(){

  // Write your code here 
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        int k=i+1;
        for(int j=0;j<n+i;j++){
            if(j<n-i-1){
                cout<<" ";
            }
            else{
                cout<<(j<n-1?k--:k++);
            }
        }
        cout<<endl;
    }

  
}

